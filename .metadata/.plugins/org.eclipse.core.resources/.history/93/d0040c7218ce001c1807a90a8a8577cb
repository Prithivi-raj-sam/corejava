package com.chainsys.Arrays;

public class NextValOfArray {
	public void main(String[] args) {
	    System.out.println("ans:");
		int [] a= {8,4,1,9,6,2};
		for (int num:a) {
			int temp=100;
			for(int num1:a) {
				if(num<num1 && num1<temp) {
				    temp=num1;
				}
			}
			System.out.println(num+"->"+temp+",");
		}
	}

}
